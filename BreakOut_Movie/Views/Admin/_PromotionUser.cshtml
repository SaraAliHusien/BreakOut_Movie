@model PromotionUserVM
<div class="d-flex justify-content-end  ">
    <a href="javaScript:;" class="close-icon-pro">
        <h3 style="color:#445754">  <i class="bi bi-x-square"></i> </h3>
    </a>
</div>

<div class="col-lg-7   Genreform">

    <div class="headers mb-5" >
        <h2>
           Promotion User
        </h2>
    </div>
    <div>
        <form class="row g-3 form-body" method="POST" asp-action="PromotionRealUser" enctype="multipart/form-data">
        <div class="form-group col-11">
            <label asp-for="UserName"></label>
            <input type="text" class="form-control" asp-for="UserName" maxlength="250" placeholder="User Name" />

            <span asp-validation-for="UserName" class="text-danger"></span>
        </div>

        <div class="p-1 col-11 d-flex justify-content-center ">
            <button type="submit" class="btn btn-primary" style="padding:6px 28px">
                <i class="bi bi-check-lg"></i>
                Promotion
            </button>
        </div>
        </form>
    </div>

</div>




